{
  "name": "robust-validator",
  "version": "0.1.1",
  "description": "Rule-based data validation library",
  "exports": {
    ".": {
      "types": {
        "default": "./dist/index.d.ts"
      },
      "browser": {
        "require": "./dist/browser/bundle.cjs",
        "default": "./dist/index.js"
      },
      "default": {
        "require": "./dist/node/bundle.cjs",
        "default": "./dist/bundle.js"
      }
    },
    "./package.json": "./package.json",
    "./dist/i18n/*": "./dist/i18n/*"
  },
  "scripts": {
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "prepare": "husky install",
    "build": "rm -rf ./dist && rollup -c"
  },
  "files": [
    "/dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "robust-validator",
    "validatorjs",
    "validator.js",
    "data validation",
    "validator",
    "validate",
    "validation",
    "data",
    "laravel",
    "laravel-validator-for-js"
  ],
  "author": "Özgür Adem Işıklı <i.ozguradem@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/axe-api/validator"
  },
  "bugs": {
    "url": "https://github.com/axe-api/validator/issues"
  },
  "homepage": "https://github.com/axe-api/validator#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-typescript": "^7.23.3",
    "@jest/globals": "^29.7.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitest/coverage-v8": "^1.1.0",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.4.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "rollup": "^4.9.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  }
}
